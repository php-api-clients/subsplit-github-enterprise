{
  "name": "api-clients/github-enterprise",
  "description": "Non-Blocking first GitHub Enterprise (v3.2) client",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "api-clients/github-enterprise-common": "^0.3",
    "api-clients/contracts": "^0.1",
    "cebe/php-openapi": "dev-openapi-31 as 1.6.0",
    "eventsauce/object-hydrator": "^1.1",
    "league/openapi-psr7-validator": "^0.18",
    "react/async": "^4.0",
    "wyrihaximus/react-awaitable-observable": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ApiClients\\Client\\GitHubEnterprise\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2.13"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "wyrihaximus/composer-update-bin-autoload-path": true,
      "infection/extension-installer": true
    }
  },
"sugest": {
"api-clients/github-enterprise-meta": "*","api-clients/github-enterprise-enterprise-admin": "*","api-clients/github-enterprise-apps": "*","api-clients/github-enterprise-oauth-authorizations": "*","api-clients/github-enterprise-codes-of-conduct": "*","api-clients/github-enterprise-emojis": "*","api-clients/github-enterprise-activity": "*","api-clients/github-enterprise-gists": "*","api-clients/github-enterprise-gitignore": "*","api-clients/github-enterprise-issues": "*","api-clients/github-enterprise-licenses": "*","api-clients/github-enterprise-markdown": "*","api-clients/github-enterprise-orgs": "*","api-clients/github-enterprise-actions": "*","api-clients/github-enterprise-projects": "*","api-clients/github-enterprise-repos": "*","api-clients/github-enterprise-teams": "*","api-clients/github-enterprise-reactions": "*","api-clients/github-enterprise-rate-limit": "*","api-clients/github-enterprise-checks": "*","api-clients/github-enterprise-code-scanning": "*","api-clients/github-enterprise-git": "*","api-clients/github-enterprise-pulls": "*","api-clients/github-enterprise-secret-scanning": "*","api-clients/github-enterprise-search": "*","api-clients/github-enterprise-users": "*"},
  "require-dev": {
    "wyrihaximus/async-test-utilities": "^5.0"
  }
}
